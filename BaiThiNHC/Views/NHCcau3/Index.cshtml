@model IEnumerable<BaiThiNHC.Models.NHCcau3>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
             <th>
                Mã sinh viên
            </th>
            <th>
                Tên sinh viên 
            </th>
            <th>
                Địa chỉ
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaSinhVien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenSinhVien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaSinhVien">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaSinhVien">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaSinhVien">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
